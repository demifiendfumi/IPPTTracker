<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.edu.rp.c346.ippttracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartCount" />
        <activity android:name=".Count_Number" />
        <activity
            android:name=".ChartImage"
            android:label="@string/title_activity_chart_image"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".IPPT_Requirement"></activity>
    </application>

</manifest>